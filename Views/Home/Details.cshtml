@model Microsoft.AspNetCore.Identity.IdentityUser

<h2>User Details</h2>

<table class="table table-bordered">
    <tr>
        <th>Email:</th>
        <td>@Model.Email</td>
    </tr>
    <tr>
        <th>Username:</th>
        <td>@Model.UserName</td>
    </tr>
    <tr>
        <th>User ID:</th>
        <td>@Model.Id</td>
    </tr>
</table>

<a href="@Url.Action("Index")" class="btn btn-secondary">Back to List</a>
